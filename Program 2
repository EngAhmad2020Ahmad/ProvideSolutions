//Task 2: Multiplication Table from 1 to 10
//Here's a program that prints the multiplication table from 1 to 10, with a blank line between each table:

using System; 
 namespace MultiplicationTable 
{ 
class Program 
{ 
static void Main(string[] args) 
{ 
for (int i = 1; i <= 10; i++) 
{
for (int j = 1; j <= 10; j++) 
{
Console.WriteLine($"{i} x {j} = {i * j}"); 
}
Console.WriteLine(); 
// Blank line between tables
} } } }
